{
  "manifest_version": 3,
  "name": "request-intercept-extension",
  "description": "Chrome Extension, TypeScript, Visual Studio Code",
  "version": "1.0.0",
  "action": {
    "default_popup": "popup.html",
    "default_icon": " icon.png"
  },
  "content_scripts": [{
    "matches": ["<all_urls>"],
    "js": ["js/content_script.js"]
  }],
  "background": {
    "service_worker": "js/background.js"
  },
  "permissions": [
    "storage",
    "cookies"
  ]
}
